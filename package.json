{
  "name": "playwright",
  "version": "1.0.0",
  "description": "Automated software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "tests:chrome": "npx playwright test --config=playwright.config.ts --project=chromium",
    "tests:firefox": "npx playwright test --config=playwright.config.ts --project=firefox",
    "tests:webkit": "npx playwright test --config=playwright.config.ts --project=webkit",
    "tests:e2e": "playwright test --config=e2e.config.ts --project=chromium --reporter=list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcsanchez1/playwright.git"
  },
  "author": "Marc Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcsanchez1/playwright/issues"
  },
  "homepage": "https://github.com/marcsanchez1/playwright#readme",
  "dependencies": {
    "@playwright/test": "^1.32.1",
    "prettier": "^2.8.7"
  }
}